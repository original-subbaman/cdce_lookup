<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>DataLoader</servlet-name>
  	<display-name>DataLoader</display-name>
  	<description></description>
  	<servlet-class>com.cdce_gmc.db.DataLoader</servlet-class>
  	<load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
  	<servlet-name>SearchVoterServlet</servlet-name>
  	<display-name>SearchVoterServlet</display-name>
  	<description></description>
  	<servlet-class>com.cdce_gmc.search.SearchVoterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>DataLoader</servlet-name>
  	<url-pattern>/DataLoader</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SearchVoterServlet</servlet-name>
  	<url-pattern>/SearchVoterServlet</url-pattern>
  </servlet-mapping>
</web-app>
